<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">

	<persistence-unit name="dryuf" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>

		<class>net.dryuf.config.DbConfigProfile</class>
		<class>net.dryuf.config.DbConfigSection</class>
		<class>net.dryuf.config.DbConfigEntry</class>

		<class>net.dryuf.security.UserAccount</class>

		<class>net.dryuf.menu.WebMenuItem</class>
		<class>net.dryuf.menu.WebAccessiblePage</class>
		<class>net.dryuf.menu.WebLanguage</class>

		<class>net.dryuf.dao.test.data.TestEnt</class>

		<class>net.dryuf.tenv.TestMain</class>
		<class>net.dryuf.tenv.TestChild</class>

		<class>net.dryuf.comp.forum.ForumHeader</class>
		<class>net.dryuf.comp.forum.ForumRecord</class>

		<class>net.dryuf.comp.gallery.GalleryHeader</class>
		<class>net.dryuf.comp.gallery.GallerySection</class>
		<class>net.dryuf.comp.gallery.GalleryRecord</class>
		<class>net.dryuf.comp.gallery.GallerySource</class>

		<class>net.dryuf.comp.poll.PollGroup</class>
		<class>net.dryuf.comp.poll.PollHeader</class>
		<class>net.dryuf.comp.poll.PollOption</class>
		<class>net.dryuf.comp.poll.PollRecord</class>

		<class>net.dryuf.comp.rating.RatingHeader</class>
		<class>net.dryuf.comp.rating.RatingRecord</class>

		<exclude-unlisted-classes />
	</persistence-unit>

</persistence>
